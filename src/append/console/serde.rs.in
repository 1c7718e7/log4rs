/// The console appender's configuration.
#[derive(Deserialize)]
#[serde(deny_unknown_fields)]
pub struct ConsoleAppenderConfig {
    target: Option<Target>,
    encoder: Option<EncoderConfig>,
}

/// The stream to log to.
#[derive(Deserialize)]
pub enum Target {
    /// Standard output.
    #[serde(rename = "stdout")]
    Stdout,
    /// Standard error.
    #[serde(rename = "stderr")]
    Stderr,
}
